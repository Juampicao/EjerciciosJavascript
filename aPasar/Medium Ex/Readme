// Regex

let patente = prompt("Ingrese una patente");
let patron = /[a-z]{2}[0-9]{3}[a-z]{2}/;
if (patron.test(patente)) alert("La patente ingresada es correcta");
else alert("La patente ingresada no es correcta");
